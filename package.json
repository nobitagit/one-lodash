{
  "name": "es6-init",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "mkdir dist; npm run build",
    "clean": "rm -rf dist",
    "build": "browserify src/index.js -o dist/bundle.js",
    "build:w": "watchify src/index.js -o dist/bundle.js"
  },
  "keywords": [],
  "author": "Nobita",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "watchify": "^3.6.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "eslint": "^3.11.1",
    "lodash": "^4.17.2",
    "ramda": "^0.22.1",
    "tape": "^4.6.3"
  }
}
